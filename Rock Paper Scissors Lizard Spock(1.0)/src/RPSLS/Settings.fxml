<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="150.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="RPSLS.Player">
   <children>
      <VBox layoutX="125.0" layoutY="33.0" prefHeight="200.0" prefWidth="100.0">
         <children>
            <TextField fx:id="inputPlayerName" />
            <Button fx:id="saveNameButton" mnemonicParsing="false" onAction="#getName" text="Enter" />
         </children>
      </VBox>
      <Label layoutX="114.0" layoutY="14.0" text="Enter your player name" />
   </children>
</AnchorPane>
